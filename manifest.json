{
    "manifest_version": 3,
    "name": "Wiktionary Context Search",
    "version": "1.0",
    "description": "Search selected text in Wiktionary based on browser language.",
    "permissions": ["contextMenus", "tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
